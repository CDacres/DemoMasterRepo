  "scripts": {
    "clean": "babel-node tools/run clean",
    "dev": "yarn run clean && yarn install && php lang_script.php && yarn run node-server:start && NODE_ENV=development webpack --watch --progress --colors",
    "dev-build": "yarn run clean && php lang_script.php && NODE_ENV=development webpack --progress --colors",
    "ci-build": "NODE_ENV=production webpack --bail",
    "build": "NODE_ENV=production webpack --progress --colors --bail",
    "build-css": "node-sass -w sass/pages -o public/css/",
    "build-server": "NODE_ENV=server webpack --progress --colors --bail",
    "prod": "yarn run node-server:stop && yarn run clean && yarn install && yarn run build && yarn run node-server:start",
    "reset": "yarn run node-server:stop && yarn run clean && git pull && yarn run build && yarn run node-server:start",
    "restart_node": "yarn run node-server:stop && yarn run node-server:start",
    "lang": "php lang_script.php",
    "node-server:start": "forever start -a --uid 'ssr-server' node-server/node-server.js --prod",
    "node-server:stop": "forever stop 'ssr-server'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
