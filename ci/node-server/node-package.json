        "redux": "^3.6.0"
    },
    "babel": {
        "presets": [
            "react",
            "latest",
            "stage-0"
        ],
        "plugins": [
            "syntax-trailing-function-commas",
            "transform-async-to-generator",
            "transform-es2015-destructuring",
            "transform-es2015-parameters",
            "transform-es2015-duplicate-keys",
            "transform-es2015-modules-commonjs",
            "transform-exponentiation-operator",
            "transform-runtime"
        ],
        "env": {
            "test": {
                "plugins": [
                    "rewire"
                ]
            }
        }
    },
    "scripts": {
        "precommit": "yarn prodTest",
        "postcommit": "yarn devBuild",
        "clean": "babel-node tools/run clean",
        "dev": "yarn run foreverStop && yarn run clean && yarn run foreverDev && NODE_ENV=development webpack --watch --verbose",
        "devBuild": "yarn run clean && NODE_ENV=development webpack --progress --colors --verbose",
        "build": "NODE_ENV=production webpack --progress --colors --bail",
        "prod": "yarn run foreverStop && yarn run clean && yarn run build && yarn run foreverProd",
        "prodTest": "yarn run clean && yarn run build",
        "test": "mocha --recursive --compilers js:babel-core/register",
        "node-server": "node ./node-server/node-server.js",
        "foreverDev": "forever start -l forever.log -a -o out.log -e err.log -w ./node-server/node-server.js",
        "foreverProd": "forever start -l forever.log -a -o out.log -e err.log ./node-server/node-server.js",
        "foreverStop": "forever stopall"
    }
}
